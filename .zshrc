alias build_dc='docker build -t projectName . '
alias run_dc='docker run -d --name projectName -p 8000:8000 projectName'
alias run_ptr='poetry run uvicorn main:app --reload --app-dir src '
alias poetry_export_req='poetry export -f requirements.txt  --output requirements.txt'
alias pipinstall='pip install -r requirements.txt'
alias coveragereport='poetry run coverage report -m'
alias manulprecommit='pre-commit run --all-files'
alias linterrun='flake8 .'
alias typecheckerrun='mypy .'